  },
  "scripts": {
    "dev": "node -r dotenv/config ./node_modules/.bin/next dev",
    "prebuild": "node scripts/ensure-prisma-generation.js",
    "build": "next build",
    "build:clean": "node scripts/clear-cache.js && npm run build",
    "build:production": "node scripts/production-build-fix.js && prisma generate && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:check": "node scripts/check-database.js",
    "db:seed": "node scripts/seed-data.js",
    "db:test": "node debug-recent-articles.js",
    "prod:check": "node scripts/production-diagnostics.js",
    "check-empty-categories": "node scripts/manage-empty-categories.js check",
    "delete-empty-categories": "node scripts/manage-empty-categories.js delete",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "check-roles": "node scripts/check-roles.js",
    "seed-roles": "node scripts/seed-roles.js",
    "publish-scheduled": "node scripts/publish-scheduled.js",
    "publish-scheduled:interactive": "node scripts/publish-scheduled.js --interactive",
    "diagnose-scheduled": "node scripts/diagnose-scheduled.js",
    "test": "jest",
    "test:babel": "jest --config=jest.config.fallback.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "artillery run performance/load-test.yml",
    "test:security": "snyk test && npm audit",
    "test:auth": "node scripts/test-auth-system-comprehensive.js",
    "test:email": "node scripts/test-email-setup.js",
    "bundle-analyze": "ANALYZE=true npm run build && open ./bundle-report.html",
    "backup": "node backup-controller.js backup",
    "backup:full": "node backup-controller.js backup --type=full",
    "backup:database": "node backup-controller.js backup --type=database",
    "backup:assets": "node backup-controller.js backup --type=assets",
    "restore": "node backup-controller.js restore",
    "backup:diagnose": "node backup-controller.js diagnose",
    "backup:status": "node backup-controller.js status",
    "backup:cleanup": "node backup-controller.js cleanup",
    "backup:schedule:start": "node backup-controller.js schedule --start",
    "backup:schedule:stop": "node backup-controller.js schedule --stop",
    "backup:setup": "node backup-controller.js setup",
    "cleanup:trial-users": "node scripts/cleanup-trial-users.js",
    "loyalty:migrate": "node scripts/migrate-loyalty-points-to-db.js",
    "loyalty:diagnose": "node scripts/fix-loyalty-system.js diagnose",
    "loyalty:fix": "node scripts/fix-loyalty-system.js fix",
    "loyalty:test": "node scripts/fix-loyalty-system.js test",
    "ai:test": "node scripts/test-openai-connection.js",
    "ai:check-production": "node scripts/check-production-ai.js",
    "ai:test-substring": "node scripts/test-substring-fix.js",
    "ai:test-summary": "node scripts/test-smart-summary-improvement.js",
    "db:phase2": "node scripts/phase2-db-setup.js",
    "perf:test": "k6 run scripts/advanced-performance-test.js",
    "perf:prod": "BASE_URL=https://www.sabq.io k6 run scripts/advanced-performance-test.js",
    "ui:test-breaking": "node scripts/test-breaking-news-toggle.js",
    "ui:test-red": "node scripts/test-breaking-news-red-color.js",
    "ui:test-persist": "node scripts/test-breaking-news-persistence.js",
    "ui:test-colors": "node scripts/test-news-counter-colors.js",
    "notifications:test": "node scripts/test-notification-persistence.js",
    "fix:react-errors": "node test-notification-fix.js && node test-notification-advanced.js"
