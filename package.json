    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "critters": "^0.0.23",
    "eslint-config-next": "^15.3.4",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prisma": "^6.10.1",
    "ts-prune": "^0.10.3",
