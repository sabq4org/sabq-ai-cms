{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "NODE_ENV=development next dev",
    "build": "node scripts/vercel-build-optimization.js && prisma generate && next build",
    "build:force": "rm -rf .next && NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prune": "ts-prune --ignoreExternal",
    "cleanup": "./scripts/cleanup.sh",
    "analyze": "cross-env ANALYZE=true next build",
    "setup-email": "node scripts/setup-email.js",
    "test-email": "node scripts/test-email.js",
    "fix-upload": "node scripts/fix-upload-permissions.js",
    "test-upload": "node scripts/test-upload-production.js",
    "build:safe": "node scripts/create-build-env.js && cp .env.build .env && next build && rm .env",
    "build:production": "SKIP_EMAIL_VERIFICATION=true next build",
    "fix-build": "node scripts/fix-build-deps.js",
    "check-deps": "node scripts/check-deps.js",
    "postinstall": "prisma generate || true",
    "db:migrate:deploy": "echo 'Run your migration tool here'",
    "db:backup": "bash scripts/backup-production.sh",
    "check:production": "node scripts/check-production-safety.js",
    "deploy": "npm run check:production && npm run build:production",
    "deploy:safe": "npm run db:backup && npm run deploy",
    "clean:production": "node scripts/clean-production-data.js",
    "clean:test-data": "node scripts/clean-production-data.js",
    "fix:permissions": "bash scripts/fix-permissions.sh",
    "fix:build": "bash scripts/quick-fix-build.sh",
    "clean:build": "rm -rf .next && rm -rf node_modules/.cache && mkdir -p .next",
    "clean:start": "./clean-start.sh",
    "fresh": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .turbo && npm run dev",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:migrate": "npx ts-node scripts/migrate-to-prisma.ts",
    "build:prod": "NODE_ENV=production ./scripts/setup-prisma.sh && next build",
    "setup:prod": "./scripts/setup-production-db.sh",
    "prisma:prod": "NODE_ENV=production npx prisma db push",
    "deploy:prod": "npm run setup:prod && npm run build:prod && npm run prisma:prod",
    "postbuild": "echo 'Build completed successfully'",
    "vercel:check": "node scripts/vercel-deploy-check.js",
    "migrate:prod": "node scripts/migrate-production.js"
  },
  "dependencies": {
